[Argument]
arg1 = switch,true,tag=Blued_VIP
arg2 = switch,false,tag=Blued位置
arg3 = switch,false,tag=翻咔查看图片
arg4 = switch,false,tag=翻咔位置

customLatitude = input, "", tag = 输入纬度, desc = 纬度
customLongitude = input, "", tag = 输入经度, desc = 经度
customCity = input, "", tag = 输入城市, desc = 你的城市
AK = input, "", tag = 输入百度地图API密钥, desc = 你的百度地图API密钥
sortBy = select, "nearby", "online", tag = 地图找人, desc = nearby附近的人，online在线的人
scriptEnabled = select, "false", "true", tag = 是否启用经纬度脚本, desc = 启用经纬度

[Rule]
URL-REGEX,https://argo.blued.cn/blued/promote.*,REJECT
URL-REGEX,https://argo.blued.cn/blued/adms/.*,REJECT
URL-REGEX,https://argo.blued.cn/blued/splash.*,REJECT
URL-REGEX,https://argo.blued.cn/blued/launch/adms,REJECT
URL-REGEX,https://social.blued.cn/users/.*/joy,REJECT
URL-REGEX,^https:\/\/social\.blued\.cn\/users\/recommend$,REJECT

[Script]
http-response ^https:\/\/.*\/users.*(setting|is_living=false|more/ios|flash|shadow|fair/exchange/count|\?aaid=|basic|map|\?column=) script-path=https://raw.githubusercontent.com/sunshine970918/RemoveAds/refs/heads/main/Loon/Plugins/22.js, requires-body=true, enable = {arg1}, tag=blued_VIP, binary-body-mode = true, argument=[{EricPassword},{scriptvip}]

http-request https://((social|argo).blued.cn|\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b)/(users.*extra_info=|ticktocks/users|groups/nearby|users/selection|ticktocks/popups|ticktocks/info|ticktocks/banner).* script-path=https://gist.githubusercontent.com/Alex0510/23b38f5ad332e30d11a60a28ee4aa132/raw/bluedlocation.js, enable = {arg2}, tag=blued_location, argument[{customLatitude},{customLongitude},{sortBy},{scriptEnabled}]

http-response https://api.finka.cn/vas/nearby/v2 script-path=https://gist.githubusercontent.com/Alex0510/85c95aeaf2283ab825702d27ed229a54/raw/Finka.js, requires-body=true, enable = {arg3}, tag=翻咔查看pic

http-request https://(gethornet.com|api.finka.cn|api.sqaces.com|api.theorangemeetingplace.com).* script-path=https://gist.githubusercontent.com/Alex0510/85c95aeaf2283ab825702d27ed229a54/raw/Finka.js, requires-body=true, enable = {arg4}, tag=Finka_location

[MITM]
hostname = api.finka.cn, gethornet.com, api.theorangemeetingplace.com, api.sqaces.com, social.blued.cn, argo.blued.cn, *.bldimg.com, ulogs.umengcloud.com, 220.249.135.81, 182.91.254.69
